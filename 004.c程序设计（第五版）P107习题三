#include<stdio.h>
int main()
{
 int a=3,b=4,c=5;
 printf("%d\n",a+b>c&&b==c);
 return 0;
}

#include<stdio.h>
int main()
{
 int a=3,b=4,c=5;
 printf("%d\n",a||b+c&&b-c);
 return 0;
}

#include<stdio.h>
int main()
{
 int a=3,b=4,c=5;
 printf("%d\n",!(a>b)&&!c||1);
 return 0;
}

#include<stdio.h>
int main()
{
 int a=3,b=4,c=5;
 int x,y;
 printf("%d\n",!(x=a)&&(y=b)&&0);
 return 0;
}

#include<stdio.h>
int main()
{
 int a=3,b=4,c=5;
 printf("%d\n",!(a+b)+c-1&&b+c/2);
 return 0;
}
